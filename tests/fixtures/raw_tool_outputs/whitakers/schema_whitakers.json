{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Whitaker's Words Backend Output",
  "description": "Raw output from Whitaker's Words backend for Latin words",
  "type": "object",
  "properties": {
    "raw": {"type": "string"},
    "parsing": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "stem": {"type": "string"},
          "ending": {"type": "string"},
          "part_of_speech": {"type": "string"},
          "word_type": {"type": "string"},
          "translation": {"type": "array", "items": {"type": "string"}},
          "grammar_info": {"type": "string"}
        }
      }
    },
    "headword": {"type": "string"},
    "error": {"type": "string"}
  }
}