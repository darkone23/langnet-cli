{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "CDSL Backend Output",
  "description": "Raw output from CDSL backend for Sanskrit words",
  "type": "object",
  "properties": {
    "dictionaries": {
      "type": "object",
      "properties": {
        "mw": {
          "type": "object",
          "properties": {
            "entries": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "headword": {"type": "string"},
                  "definitions": {"type": "array", "items": {"type": "string"}},
                  "grammar": {"type": "string"}
                }
              }
            }
          }
        },
        "ap90": {
          "type": "object",
          "properties": {
            "entries": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "headword": {"type": "string"},
                  "definitions": {"type": "array", "items": {"type": "string"}},
                  "grammar": {"type": "string"}
                }
              }
            }
          }
        }
      }
    },
    "_search_method": {"type": "string"},
    "_warning": {"type": "string"},
    "error": {"type": "string"}
  }
}